generator client {
  provider      = "prisma-client"
  output        = "./generated"
  binaryTargets = ["native", "linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model movie {
  id          Int    @id
  title       String @db.VarChar(255)
  releaseYear Int    @map("release_year")
  actorIds    Int[]  @map("actor_ids")
}
